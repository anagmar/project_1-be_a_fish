<form action="/divers" method="post">

  <label for="name-field">Name:</label>
  <input
    type="text"
    name="name"
    id ="name-field"
    />

  <label for="level-select">Choose your diving level:</label>
  <select name="level" id="level-select">
    <option value="Open Water">Open Water</option>
    <option value="Advanced">Andvanced</option>
    <option value="Master Diver">Master Diver</option>
  </select>

  <input type="submit" value="New Diver">
  </form>
